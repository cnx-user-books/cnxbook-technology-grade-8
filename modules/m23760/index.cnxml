          <tbody>
            <row>
              <entry>Final product:</entry>
              <entry>Mark Out of 10</entry>
            </row>
            <row>
              <entry>
                <list id="id20721104" list-type="bulleted">
                  <item>Cultural link</item>
                </list>
              </entry>
              <entry/>
            </row>
            <row>
              <entry>
                <list id="id13877213" list-type="bulleted">
                  <item>Nutritional value</item>
                </list>
              </entry>
              <entry/>
            </row>
